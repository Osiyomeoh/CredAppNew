 = devisor_for_minutes % 60;\\r\\n    seconds = Math.ceil(devisor_for_seconds);\\r\\n\\r\\n    let obj = {\\r\\n      h: hours,\\r\\n      m: minutes,\\r\\n      s: seconds,\\r\\n    };\\r\\n    return obj;\\r\\n  }\\r\\n\\r\\n  componentDidMount() {\\r\\n    let timeLeftVar = this.secondstoTime(this.state.seconds);\\r\\n    this.setState({ time: timeLeftVar });\\r\\n  }\\r\\n\\r\\n  airdropReleaseTokens() {\\r\\n    let stakingB = this.props.stakingBalance;\\r\\n    if (stakingB >= \\\"1000000000000000000\\\") {\\r\\n      // If staking balance is more than 50 usdt, begin the airdrop of reward token.\\r\\n      this.startTimer();\\r\\n    }\\r\\n  }\\r\\n\\r\\n  render() {\\r\\n    this.airdropReleaseTokens();\\r\\n    return (\\r\\n      <div style={{ color: \\\"white\\\" }}>\\r\\n        {this.state.time.m}:{this.state.time.s}\\r\\n      </div>\\r\\n    );\\r\\n  }\\r\\n}\\r\\n\\r\\nexport default Airdrop;\\r\\n\",\"import abi from './Transactions.json'\\r\\nimport CredToken from './CredToken.json'\\r\\nimport Gains from './Gains.json'\\r\\nimport TokenFarm from './TokenFarm.json'\\r\\n\\r\\n\\r\\n\\r\\nexport const contractAddress = '0x91770151aB0e5CBEDf771baAdAb9647BD4007888';\\r\\nexport const contractABI = abi.abi; \\r\\n\\r\\nexport const CredTokenAddress = '0x1A9e8DAeA93FfCa2D97BdBf32a67c4d70D785C45';\\r\\nexport const CredTokenABI = CredToken.abi; \\r\\n\\r\\nexport const GainsAddress = '0x721975aD8dE10A14FA2D6C4EfAB406979e7DF5eA';\\r\\nexport const GainsABI = Gains.abi; \\r\\n\\r\\nexport const TokenFarmAddress = '0x3AA7ccd2C2fF20AC7bB62591aAaef7353c7Cc32A';\\r\\nexport const TokenFarmABI = TokenFarm.abi; \\r\\n\\r\\n\\r\\n// credToken: 0x67abAB6Fd6da88692ffa29ee4e87Bea00f6A0A4e\\r\\n// testToken :0xAe23adF7BC41F40117d7e3369239387eb15bF05B\\r\\n// TokenFarm: 0x2B45B69c3f9784844b70bE08dfBb71a8C31657a2\\r\\n\",\"import React, { useEffect, useState } from \\\"react\\\";\\r\\nimport { ethers } from \\\"ethers\\\";\\r\\n\\r\\nimport { contractABI, contractAddr